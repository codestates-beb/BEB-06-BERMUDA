CREATE TABLE `user` (
  `user_id` varchar(255),
  `password` varchar(255),
  `nickname` varchar(255),
  `address` varchar(255),
  `private_key` varchar(255),
  `token_amount` int,
  `eth_amount` int,
  `isVoted` boolean,
  `created_at` timestamp
);

CREATE TABLE `nft` (
  `user_id` int,
  `token_id` int,
  `tx_hash` varchar(255)
);

CREATE TABLE `vote` (
  `tournament` varchar(255),
  `num_one` int,
  `num_two` int
);

ALTER TABLE `nft` ADD FOREIGN KEY (`user_id`) REFERENCES `user` (`id`);
